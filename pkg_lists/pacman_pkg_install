# Install Pacman Stuff
Programs="
git-delta
alacritty
git
gimp
zsh
zsh-completions
copyq
xclip
firefox
ripgrep
cmake
base-devel
make
libffi
libyaml
openssl
zlib
java11-openjdk
pulseaudio-bluetooth
virtualbox-guest-iso
virtualbox
keychain
exa
yarn
grc
owncloud-client
thunderbird
dnsutils
xournalpp
noto-fonts-emoji
k9s
keepassxc
ctags
"
for val in $Programs; do
    pacman -S --needed --noconfirm $val
done
